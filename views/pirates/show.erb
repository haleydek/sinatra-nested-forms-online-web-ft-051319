<h1>Pirate Name: <%= @pirate.name %></h1>
  <li>Weight: <%= @pirate.weight %></li>
  <li>Height: <%= @pirate.height %></li>
<br>
<% @ships.each do |ship_hash| %>
  <h2>Ship Name: <%= ship_hash["name"] %></h2>
    <li>Type: <%= ship_hash["type"] %></li>
    <li>Booty: <%= ship_hash["booty"] %></li>
<% end %>
